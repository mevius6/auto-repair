nav.nav#Nav
  button.nav__toggle
    | Меню
  ul.nav__items(aria-label="Main Menu")
    each navItem, i in navItems
      if navItem.subItems
        li.nav__item
          a.nav__link(
            id=`item0${i+1}`
            href=`${appData.meta.domain}/${navItem.pathname}`
            rel="noopener" target="_blank"
          )
            span.nav__link-text
              | #{navItem.title}
          button(
            type="button"
            id=`btnItem0${i+1}`
            aria-controls=`SubItem0${i+1}`
            aria-expanded="false"
            aria-label="Show"
            aria-labelledby=`btnItem0${i+1} item0${i+1}`
            onclick="toggleSubNav(this.id);"
          )
            svg(xmlns="http://www.w3.org/2000/svg" viewBox="0 0 80 80")
              path(d="M70.3 13.8L40 66.3 9.7 13.8z")
          ul.nav__subitems.flow(id=`SubItem0${i+1}` style={'display': 'none'})
            each subItem, i in navItem.subItems
              li.nav__subitem
                a.nav__link(
                  href=`${appData.meta.domain}/${subItem.pathname}`
                  rel="noopener" target="_blank"
                )
                  span.nav__link-text
                    | #{subItem.title}
      else
        li.nav__item(class={'selected': (i+1) === 4})
          a.nav__link(
            id=`item0${i+1}`
            href=`${appData.meta.domain}/${navItem.pathname}`
            rel="noopener" target="_blank"
          )
            span.nav__link-text
              | #{navItem.title}
